#include<stdio.h>
int search(int * array,int query,int start,int end)
{
int mid;
while(start<=end)
{
	mid=(start+end)/2;
	if(array[mid]==query)
		return mid;
	else if(array[mid]<query)
		start=mid+1;
	else
		end=mid-1;
}
return -1;
}
int main()
{
int n;
int arr[]={1,3,6,8,10,12,40,100,120};
scanf(" %d",&n);
printf("%d\n",search(a,n,0,8);
return 0;
}
